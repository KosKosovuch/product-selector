{
  "name": "product-selector",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open --port 3333",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write --list-different .",
    "fix:eslint": "eslint --ext .ts,.js,.vue . --ignore-path .gitignore . --fix",
    "fix": "npm-run-all fix:*",
    "lint": "npm-run-all lint:*",
    "ct": "git add . && npx git-cz"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "pinia": "^2.0.35",
    "roboto-fontface": "*",
    "sass": "^1.62.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.0",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
